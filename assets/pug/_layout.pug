include templates/_global_vars
include templates/_global_mixins
include templates/_project_mixins


- array = '';
block vars
doctype
html(lang="en")
	head
		+head(array)
	body(class= array.body_class)
		include interface/header
		.wrapper(class= array.layout_mod)
			.base(class= array.layout_mod)
					block content_block
			include interface/footer
		+scripts
		block local_popup
			include interface/popup
			include interface/succes
			include interface/close
